#!/bin/bash

EXP_NAME='TriviaQA-Untrained'
META='TriviaQA Accuracy without extra training, just SQuAD trained model'

DOCKERIZE=0
BACKUP=0

BATCH_SIZE=12
SEQ_LENGTH=384

BERT_DIR=`pwd`/models/uncased
MODEL_DIR=`pwd`/squad_output
_BASE_DIR=`pwd`/experiments/triviaqa_experiment

_QA_DIR=$_BASE_DIR/triviaqa_squad
OUTPUT_DIR=$_BASE_DIR/triviaqa_output

TRAIN_FILE=$_QA_DIR/wikipedia-train.json
PREDICT_FILE=$_QA_DIR/wikipedia-dev.json

CHECKPOINT=$MODEL_DIR/model.ckpt-16289
TRAIN=False
THRESH='-2.1419568061828613'
